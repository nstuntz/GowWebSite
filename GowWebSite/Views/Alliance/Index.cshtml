@model IEnumerable<GowWebSite.Models.Alliance>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Kingdom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CityX)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CityY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Kingdom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CityX)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CityY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AllianceID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AllianceID })
        </td>
    </tr>
}

</table>
