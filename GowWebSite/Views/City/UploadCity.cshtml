@model Dictionary<int,string>
@{
    ViewBag.Title = "City List";
}


<div class="col-md-12">
    @using (Html.BeginForm("UploadCity", "City", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="row">
            <div class="col-md-6">
                <input type="file" name="FileUpload1" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <p><input type="submit" name="Submit" id="Submit" value="Upload" class="btn btn-primary" /></p>
            </div>
        </div>
        if (Model.Count > 0)
        {
            <table class="table">
                <tr>
                    <th>Row Number</th>
                    <th>Errors</th>
                </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Key
                    </td>
                    <td>
                        @item.Value
                    </td>
                </tr>
            }
            </table>
        }
    }
</div>

