@model GowWebSite.Models.City


<div class="modal-content imagesModal">
    <div class="modal-header text-center">
        <h4>Images - @Model.CityName</h4>
    </div>
    <div class="modal-body">

        @foreach (KeyValuePair<string, string> image in ViewBag.Images)
        {
            <ul class="list-group list-inline">
                <li>@image.Value</li>
                <li><img class="img-responsive" src='@image.Key' /></li>
            </ul>
        }
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary modal-close-btn" data-dismiss="modal" id="approve-btn">Close</button> 
    </div>
</div>
