<script>
    
    var HelmRangedAttackLow = 0; 
    var HelmRangedAttackHigh = 0; 
    var HelmCalvaryAttackLow = 0; 
    var HelmCalvaryAttackHigh = 0; 
    var HelmInfantryAttackLow = 0; 
    var HelmInfantryAttackHigh = 0; 
    var HelmTroopAttackLow = 0; 
    var HelmTroopAttackHigh = 0; 
    var HelmTroopDefenseLow = 0; 
    var HelmTroopDefenseHigh = 0; 
    var HelmTroopHealthLow = 0; 
    var HelmTroopHealthHigh = 0; 
    var HelmAttackDebufLow = 0; 
    var HelmAttackDebuffHigh = 0; 
    var HelmHealthDebuffLow = 0; 
    var HelmHealthDebuffHigh = 0; 
    var HelmDefenseDebuffLow = 0; 
    var HelmDefenseDebuffHigh = 0; 
    var HelmMarchSpeedLow = 0; 
    var HelmMarchSpeedHigh = 0;

    var FullSummaryRangedAttackLow = 0; 
    var FullSummaryRangedAttackHigh = 0; 
    var FullSummaryCalvaryAttackLow = 0; 
    var FullSummaryCalvaryAttackHigh = 0; 
    var FullSummaryInfantryAttackLow = 0; 
    var FullSummaryInfantryAttackHigh = 0; 
    var FullSummaryTroopAttackLow = 0; 
    var FullSummaryTroopAttackHigh = 0; 
    var FullSummaryTroopDefenseLow = 0; 
    var FullSummaryTroopDefenseHigh = 0; 
    var FullSummaryTroopHealthLow = 0; 
    var FullSummaryTroopHealthHigh = 0; 
    var FullSummaryAttackDebufLow = 0; 
    var FullSummaryAttackDebuffHigh = 0; 
    var FullSummaryHealthDebuffLow = 0; 
    var FullSummaryHealthDebuffHigh = 0; 
    var FullSummaryDefenseDebuffLow = 0; 
    var FullSummaryDefenseDebuffHigh = 0; 
    var FullSummaryMarchSpeedLow = 0; 
    var FullSummaryMarchSpeedHigh = 0;

    $(function(){
        $('#nav').hover(function(){
            $(this).animate({width:'500px'},500);
        },function(){
            $(this).animate({width:'35px'},500);
        }).trigger('mouseleave');
    });

    var model = @Html.Raw(Json.Encode(@ViewBag.Cores));
    var modelPiece = @Html.Raw(Json.Encode(@ViewBag.Pieces));

    $(function () {
        $("[name='HelmCores']").change(function () {
            if (model[document.getElementById("HelmCores").selectedIndex-1].RangedAttackLow == model[document.getElementById("HelmCores").selectedIndex-1].RangedAttackHigh)
            {
                document.getElementById("HelmCoreRangedAttack").innerHTML= model[document.getElementById("HelmCores").selectedIndex-1].RangedAttackLow;
            } else{
                document.getElementById("HelmCoreRangedAttack").innerHTML= model[document.getElementById("HelmCores").selectedIndex-1].RangedAttackLow + "-" + model[document.getElementById("HelmCores").selectedIndex-1].RangedAttackHigh;
            }
            if (model[document.getElementById("HelmCores").selectedIndex-1].CalvaryAttackLow == model[document.getElementById("HelmCores").selectedIndex-1].CalvaryAttackHigh)
            {
                document.getElementById("HelmCoreCalvaryAttack").innerHTML= model[document.getElementById("HelmCores").selectedIndex-1].CalvaryAttackLow;
            } else{
                document.getElementById("HelmCoreCalvaryAttack").innerHTML= model[document.getElementById("HelmCores").selectedIndex-1].CalvaryAttackLow + "-" + model[document.getElementById("HelmCores").selectedIndex-1].CalvaryAttackHigh;
            }
